<div class="container" style="top:70px;">
<header>
<h1>Jobs Posted</h2>
<h3>Employer Id {{emp_id}}</h3> <br>
</header>
<a href="/employer/jobs"><button type="button" class="butt butt1" id="submit">Posted Jobs</button></a>
<a href="/employer/requests"><button type="button" class="butt butt1" id="submit">Requests</button></a>
<a href="/employer/confirmed"><button type="button" class="butt butt1" id="submit">Confirmed</button></a>
<a href="/employer/logout"><button style="float:right" type="button" class="butt butt1" id="submit">Logout</button></a>
<table class="custom">
<thead>
    <tr>
        <th>EmpID</th>
        <th>Type of Work</th>
        <th>Duration</th>
        <th>Wage</th>
        <th>Location</th>
    </tr>
	</thead>
	<tbody>
    {{#each jobsposted1}}
    <tr>
        <td>{{this.emp_id}}</td>
        <td>{{this.type_work}}</td>
        <td>{{this.duration.days}} Days, {{this.duration.months}} Months</td>
        <td>{{this.wage}}</td>
        <td>{{this.location}}</td>
    </tr>
    {{/each}}
	</tbody>
</table>
<br>
<div align="center" ><a href="/employer/addjob"><button type="button" class="butt butt1" id="submit">Add Jobs</button></a></div><br><br>
<br>
</div>